<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0    https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<server>
			<id>Anypoint-Platform</id>
			<name>Anypoint-Platform</name>
			<username>${ch.user}</username>
			<password>${ch.password}</password>
		</server>
		<server>
			<id>MuleRepository</id>
			<username></username>
			<password></password>
		</server>
		<server>
			<id>mulesoft-snapshot</id>
			<username>admin</username>
			<password>admin123</password>
		</server>
		<server>
			<id>mulesoft-release</id>
			<username>admin</username>
			<password>admin123</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>custom</id>
			<repositories>
				<repository>
					<id>Anypoint-Platform</id>
					<name>Anypoint-Platform</name>
					<url>https://maven.anypoint.mulesoft.com/api/v2/organizations/${group.id}/maven</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>MuleRepository</id>
					<name>MuleRepository</name>
					<url>https://repository.mulesoft.org/nexus-ee/content/repositories/releases-ee/</url>
					<layout>default</layout>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>mulesoft-releases</id>
					<name>MuleSoft Repository</name>
					<url>https://repository.mulesoft.org/releases/</url>
					<layout>default</layout>
				</repository>
			</repositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>custom</activeProfile>
	</activeProfiles>
</settings>
